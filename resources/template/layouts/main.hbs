<!DOCTYPE html>
{{> include/_copyright-header}}
<html lang="en">
	<head>
		{{> include/_head}}

		{{!-- Optional: page related CSS --}}
		{{#>head-block}}{{/head-block}}
	</head>
	<body class="mod-bg-1 {{preemptiveclass}}">
		
		{{!-- save-settings-script --}}
		{{> include/_scripts-loading-saving}}
		
		<!-- BEGIN Page Wrapper -->
		<div class="page-wrapper">

			<div class="page-inner">

				<!-- BEGIN Left Aside -->
				{{> include/_left-panel }}
				<!-- END Left Aside -->

				<div class="page-content-wrapper">
					
					<!-- BEGIN Page Header -->
					{{> include/_page-header }}
					<!-- END Page Header -->

					<!-- BEGIN Page Content -->
					<!-- the #js-page-content id is needed for some plugins to initialize -->
					<main id="js-page-content" role="main" class="page-content">
						{{#if preemptiveclass}}
							<!-- Page heading removed for composed layout -->
						{{else}}
							{{> include/_page-breadcrumb }}
							{{> include/_page-heading }}
						{{/if}}
						{{#>content-block}}{{/content-block}}
					</main>
					{{> include/_page-content-overlay }}
					<!-- END Page Content -->

					<!-- BEGIN Page Footer -->
					{{> include/_page-footer }}
					<!-- END Page Footer -->

					<!-- BEGIN Shortcuts -->
					{{> include/_shortcut-modal }}
					<!-- END Shortcuts -->

				</div>

			</div>

		</div>
		<!-- END Page Wrapper -->

		<!-- BEGIN Quick Menu -->
		{{> include/_shortcut-menu }}
		<!-- END Quick Menu -->

		<!-- BEGIN Messenger -->
		{{> include/_shortcut-messenger }}
		<!-- END Messenger -->

		<!-- BEGIN Page Settings -->
		{{> include/_page-settings }}
		<!-- END Page Settings -->

		{{!-- Google Analytics  code if GOOGLE_ANALYTICS_ID id available --}}
		{{#if GOOGLE_ANALYTICS_ID}}
			<script>
			  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

			  ga('create', '{{GOOGLE_ANALYTICS_ID}}', 'auto');
			  ga('send', 'pageview');

			</script>
		{{/if}}
		{{> include/_scripts-base-plugins}}
		{{#>scripts-block}}
			{{!-- Custom scripts per page can be added. --}}
		{{/scripts-block}}
	</body>
</html>